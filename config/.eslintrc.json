{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "app",
          "app/src"
        ]
      }
    }
  },
  "rules": {
    "no-underscore-dangle": ["error", {"allowAfterThis": true}]
  }
}
